{
    "name": "alazzi-az/odoo-xmlrpc",
    "description": "PHP package that provides a simple and easy-to-use interface for interacting with the Odoo XML-RPC API",
    "keywords": [
        "alazzi-az",
        "odoo-php",
        "odoo",
        "odoo-xmlrpc",
        "php-xmlrpc"
    ],
    "homepage": "https://github.com/alazzi-az/odoo-xmlrpc",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AlazziAz\\OdooXmlrpc\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohammed Ali Azman",
            "email": "56982649+mohammedazman@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laminas/laminas-xmlrpc": "^2.16"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "pestphp/pest": "^2.0.0",
        "brianium/paratest": "^7.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test:unit": "pest --colors=always"
    }
}
